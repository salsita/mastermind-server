{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "comma-dangle": 0,
    "func-names": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-else-return": 0,
    "global-require": 0,
    "require-yield": 0,
    "import/prefer-default-export": 0,
    "jsx-a11y/no-static-element-interactions": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack/webpack.config.dev.babel.js"
      }
    }
  }
}